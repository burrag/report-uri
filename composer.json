{
    "name": "burrag/csp-report",
    "description": "Project for csp report",
    "authors": [
        {
            "name": "Marek Humpol√≠k",
            "email": "marek.humpolik@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/Exception/exceptions.php"]
    },
    "require": {
        "mindplay/middleman": "^3.0",
        "laminas/laminas-diactoros": "^2",
        "laminas/laminas-httphandlerrunner": "^2",
        "middlewares/request-handler": "^v2.0.0",
        "symfony/config": "^4.1",
        "nette/di": "^2.4",
        "contributte/psr11-container-interface": "^0.1.0",
        "nette/bootstrap": "^2.4",
        "tracy/tracy": "^2.5",
        "nette/utils": "^2.5"
    },
    "scripts": {
        "cs": "phpcs --standard=vendor/inspirecz/coding-standard/ruleset.strict.xml --extensions=php --tab-width=4 -sp app",
        "cs-fix": "phpcbf --standard=vendor/inspirecz/coding-standard/ruleset.strict.xml --extensions=php --tab-width=4 -sp app"
    }
}
